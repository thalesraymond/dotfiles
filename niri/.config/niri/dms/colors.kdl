layout {
    background-color "transparent"

    focus-ring {
        active-color   "#ffb691"
        inactive-color "#a28c82"
        urgent-color   "#ffb4ab"
    }

    border {
        active-color   "#ffb691"
        inactive-color "#a28c82"
        urgent-color   "#ffb4ab"
    }

    shadow {
        color "#00000070"
    }

    tab-indicator {
        active-color   "#ffb691"
        inactive-color "#a28c82"
        urgent-color   "#ffb4ab"
    }

    insert-hint {
        color "#ffb69180"
    }
}